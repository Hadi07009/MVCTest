@model HelloWorld.ViewModel.EmployeeListViewModel
@*@model HelloWorld.ViewModel.EmployeeViewModel*@
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListOfEmployee</title>
</head>
<body>
    <div>

        <table style="width: 100%;">
            <tr>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
            </tr>
            @foreach (var item in Model.ListOfEmployee)
            {
                <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Salary</td>
            </tr>
            }
            <tr>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
                <td>&nbsp;</td>
            </tr>
        </table>
    </div>
</body>
</html>
